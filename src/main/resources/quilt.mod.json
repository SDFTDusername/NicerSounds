{
  "schema_version": 1,

  "quilt_loader": {
    "group": "${mod_group}",
    "id": "${mod_id}",
    "version": "${mod_version}",

    "intermediate_mappings":"net.fabricmc:intermediary",

    "metadata": {
      "name": "${mod_name}",
      "description": "Adds footstep sounds and placing and breaking sounds.",
      "contributors": {
        "SDFTDusername": "Programmer",
        "Presence Footsteps Mod": "Sound effects",
        "Terraria": "Inventory open and close sound"
      },

      "contact": {
        "homepage": "https://sdftdusername.github.io/",
        "issues": "https://github.com/SDFTDusername/NicerSounds/issues",
        "sources": "https://github.com/SDFTDusername/NicerSounds/"
      },

      "icon": "assets/nicersounds/icon.png"
    },

    "entrypoints": {
      "init": "io.github.sdftdusername.nicersounds.NicerSounds"
    },

    "depends": [
      {
        "id": "cosmicquilt",
        "versions": "*"
      },
      {
        "id": "cosmicreach",
        "versions": "*"
      }
    ]
  },

  "mixin": "nicersounds.mixins.json"
}
