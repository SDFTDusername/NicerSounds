{
  "schema_version": 1,

  "quilt_loader": {
    "group": "${mod_group}",
    "id": "${mod_id}",
    "version": "${mod_version}",

    "intermediate_mappings":"net.fabricmc:intermediary",

    "metadata": {
      "name": "${mod_name}",
      "description": "Adds footstep sounds and placing and breaking sounds.",
      "contributors": {
        "SDFTDusername": "Programmer",
        "Presence Footsteps Mod": "Sound effects",
        "Terraria": "Inventory open and close sound"
      },

      "contact": {
        "homepage": "https://example.com/",
        "issues": "https://codeberg.org/CRModders/cosmic-quilt-example/issues",
        "sources": "https://codeberg.org/CRModders/cosmic-quilt-example/"
      },

      "icon": "assets/nicer_sounds/icon.png"
    },

    "entrypoints": {
      "init": "com.sdftdusername.nicer_sounds.NicerSoundsMod"
    },

    "depends": [
      {
        "id": "cosmicquilt",
        "versions": "*"
      },
      {
        "id": "cosmicreach",
        "versions": "*"
      }
    ]
  },

  "mixin": "nicer_sounds_mod.mixins.json"
}
